/**
 * @file
 * Code for the NYC Data Demo feature.
 */

include_once 'data_story.features.inc';

/**
 * Implements hook_node_view().
 */
function data_story_node_view($node, $view_mode) {
  if ($view_mode != 'rss') {
    if ($node->type == 'blog') {
      unset($node->content['links']['blog']);
    }
  }
}
